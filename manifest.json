{
	"manifest_version": 2,
	"name": "Bitpreco Auto Trader",
	"version": "0.1",

	"description": "plaly a sound when bitcoins goes bellow the desired value",

	"background": {
		"scripts": ["bitcoin_auto_trader.js"]
	},

	"icons": {
		"48": "Icons/icon.svg",
		"96": "Icons/icon.svg"
	},

	"web_accessible_resources": [
    	"Audios/*.mp3"
 	],

 	"permissions": [
  		"tabs",
  		"activeTab",
  		"<all_urls>"
	]
}